<!DOCTYPE html>
<html>
  <head>
    <title>HealthSmartCity</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <div class="navbar navbar-inverse">
      <div class="container">
        <nav>
          <ul class="nav navbar-nav navbar-left">
            <li> <%= link_to "Home", root_path %></li>
            <li> <%= link_to "About", root_path %></li>
            <li> <%= link_to "Contact", root_path %></li>
          </ul>
          <ul class="nav navbar-nav navbar-right">
            <li> <%= link_to "Login", root_path %></li>
          </ul>
        </nav>
      </div>
    </div>
    <div class="container">
      <div class="row">
        <%= yield %>

        <script async defer
            src="https://maps.googleapis.com/maps/api/js?key=AIzaSyC3nPH6fHf4j-Dmt_e9Z-5cvlJEvVDi6Ss&callback=initialize">
        </script>
        <script src="https://www.gstatic.com/charts/loader.js"></script>
      </div>
    </div>
  </body>
</html>
